    "# ex.1\n",
    "def fib(n):\n",
    "    if n==0 or n==1:\n",
    "        return n\n",
    "    else:\n",
    "        ans0, ans1 = 0,1\n",
    "        for i in range (2,n+1):\n",
    "            ans2 = ans0 + ans1\n",
    "            ans0 = ans1\n",
    "            ans1 = ans2\n",
    "        return ans1"
   ]
